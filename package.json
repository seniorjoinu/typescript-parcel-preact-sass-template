{
  "name": "rtp-landing",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "watch:tsc": "tsc --noEmit --watch",
    "watch:parcel": "parcel index.html",
    "start": "npm-run-all --parallel watch:tsc watch:parcel",
    "build": "rm -rf dist && parcel build index.html --public-url './'"
  },
  "dependencies": {
    "preact": "8.5.2"
  },
  "devDependencies": {
    "autoprefixer": "9.6.1",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.3",
    "postcss-modules": "1.4.1",
    "sass": "1.22.10",
    "tslint": "5.18.0",
    "typescript": "3.5.3",
    "typescript-plugin-css-modules": "1.2.1"
  }
}
